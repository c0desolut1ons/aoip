<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_lib_a_rl_root"
    android:layout_width="match_parent"
    android:layout_height="@dimen/library_activity_aoip_panel_height"
    android:animateLayoutChanges="true"
    android:background="@null">

    <com.c0desolut1ons.aoiplibrary.AndroidObjectItemPickerListView
        android:id="@+id/id_lib_a_lv_list_picker"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:listSelector="@drawable/aoip_list_selector"
        style="@style/AndroidOneItemPicker.ListView"/>

    <View
        android:id="@+id/picker_line_top"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_top_line_top_pickerui"
        style="@style/line_center_default"
        />

    <View
        android:id="@+id/picker_line_bottom"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_top_line_bottom_pickerui"
        style="@style/line_center_default"
        />


</RelativeLayout>